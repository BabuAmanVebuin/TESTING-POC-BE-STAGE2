{
  "name": "nodejs-backend-template",
  "version": "1.0.0",
  "main": "index.js",
  "type": "commonjs",
  "contributors": [
    {
      "name": "Tomoki Shigeta"
    }
  ],
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "postinstall": "husky",
    "test": "jest",
    "test:reporter": "yarn jest --ci",
    "test:generate": "yarn ts-node autoGenerateTests.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format:write": "prettier . --write",
    "build": "/usr/bin/env sh scripts/build.sh",
    "start:dev": "set TS_NODE=1 && nodemon --exec node --loader ts-node/esm -r dotenv-safe/config ./index.ts",
    "start": "yarn build && node build/index.js",
    "testIntegration:cleandb": "mocha --loader=ts-node/esm -r dotenv/config -r src/infrastructure/orm/sqlize/mochaRootHook.ts -r src/infrastructure/orm/snowflake/mochaRootHook.ts 'test/test4o/integration/*.ts'",
    "testIntegrationSpecific": "/usr/bin/env bash scripts/testIntegrationSpecificWithoutDBRefresh.sh",
    "testIntegrationSpecific:cleandb": "/usr/bin/env bash scripts/testIntegrationSpecific.sh",
    "testFile": "RUNNING_UNIT_TESTS=1 mocha --loader=ts-node/esm -r dotenv/config -r src/infrastructure/orm/sqlize/mochaRootHook.ts -r src/infrastructure/orm/snowflake/mochaRootHook.ts  --timeout 25000",
    "image:remove": "./docker/script/remove-images.sh",
    "container:start": "./docker/script/start-containers.sh",
    "container:start:rebuild": "yarn image:remove && yarn container:start",
    "typeorm-cli": "ts-node ./node_modules/typeorm/cli -f ./src/infrastructure/orm/typeorm/config/ormconfig.ts",
    "importDefectNotification": "ts-node -r dotenv-safe/config importDefectNotification.ts",
    "importMaintenancePlan": "ts-node -r dotenv-safe/config importMaintenancePlan.ts"
  },
  "dependencies": {
    "@azure/event-hubs": "^5.12.2",
    "@azure/eventhubs-checkpointstore-blob": "^1.0.1",
    "@azure/storage-blob": "^12.11.0",
    "@types/lodash": "^4.14.194",
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "dotenv-safe": "^9.1.0",
    "express": "^4.21.1",
    "fp-ts": "^2.12.3",
    "i18n": "^0.15.1",
    "io-ts": "^2.2.18",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "morgan": "^1.10.0",
    "mysql2": "^3.11.3",
    "node-cron": "^3.0.2",
    "openai": "^4.84.0",
    "sequelize": "^6.21.4",
    "snowflake-sdk": "^1.7.0",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "winston": "^3.8.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.9",
    "@babel/preset-typescript": "^7.25.9",
    "@eslint/compat": "^1.2.2",
    "@stylistic/eslint-plugin": "^2.10.1",
    "@types/chai": "^5.0.1",
    "@types/cors": "^2.8.10",
    "@types/express": "^5.0.0",
    "@types/i18n": "^0.13.2",
    "@types/luxon": "^3.4.2",
    "@types/mocha": "^10.0.1",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.17.0",
    "@types/node-cron": "^3.0.9",
    "@types/sinon": "^17.0.3",
    "@types/snowflake-sdk": "^1.6.13",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.14.0",
    "chai": "^5.1.2",
    "eslint": "^9.14.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^15.2.10",
    "mocha": "^10.2.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "sinon": "^19.0.2",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
}
